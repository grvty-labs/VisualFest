.downbar {
  align-items: center;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  lost-column: 19/19 0;
  margin-bottom: 15px;
  position: fixed;

  & .dblinks {
    display: flex;
    margin-right: 100px;

    & > a {
      display: inherit;
      margin-left: 10px;
      height: 30px;
      width: 30px;
    }

    & img {
      align-items: center;
      max-height: 100%;
      max-width: 100%;
    }
  }

  & .dbline {
    border-top: 2px solid #fff;
    height: 2px;
    margin-left: 100px;
    width: 5%;
  }
}
